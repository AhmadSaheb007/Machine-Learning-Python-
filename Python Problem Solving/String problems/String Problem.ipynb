    "def findStringLen(st):\n",
    "    index = 0;\n",
    "    for char in st:\n",
    "        index += 1\n",
    "    return index\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print(findStringLen(\"abeer\"))\n",
    "    print(findStringLen('PyCharm'))\n"
